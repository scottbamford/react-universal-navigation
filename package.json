{
  "name": "react-universal-navigation",
  "version": "2.1.0",
  "description": "Universal access to navigation properties and methods for different react navigators including react-navigation (for react-native) and connected-react-router for react-redux.",
  "main": "dist/lib/commonjs/index.js",
  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/react-universal-navigation.git"
  },
  "keywords": [
    "react",
    "navigation",
    "react-native",
    "universal",
    "navigator",
    "cross-platform",
    "crossplatform"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scottbamford/react-universal-navigation/issues"
  },
  "homepage": "https://github.com/scottbamford/react-universal-navigation/#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && tsc -m esnext --outDir ./dist/lib/esm",
    "rebuild": "npm run-script clean && npm run-script build"
  },
  "dependencies": {
    "react": "^16.8.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.16",
    "typescript": "3.4.5",
    "rimraf": "^2.6.3"
  }
}
